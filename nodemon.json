{
  "env": {
    "NODE_ENV": "development"
  },
  "exec": "cross-env TS_NODE_TRANSPILE_ONLY=true node --inspect -r ts-node/register server/index.js",
  "ignore": [
    "node_modules",
    ".vscode",
    ".git",
    ".eslintrc",
    "package.json"
  ],
  "verbose": true,
  "ext": "ts, js",
  "watch": ["server"]
}
