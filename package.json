{
  "name": "react-typescript-template",
  "version": "0.2.0",
  "author": "Daniel Zhang",
  "private": true,
  "main": "server/index.js",
  "scripts": {
    "start": "nodemon server/index.js",
    "start:i": "nodemon --inspect server/index.js",
    "start:api": "cross-env API_ONLY=true nodemon server/index.js",
    "serve": "cross-env TS_NODE_TRANSPILE_ONLY=true NODE_ENV=production node server/index.js",
    "build:dev": "webpack --config webpack.vendor.js --progress --colors",
    "build:prod": "webpack --config webpack.config.prod.js --progress --colors",
    "clean:build": "rm -rf dist/bundle && rm -rf .awcache",
    "clean:node": "yarn cache clean && rm -rf node_modules && yarn",
    "cbs": "yarn clean && yarn build && yarn serve",
    "test": "jest --config=test/jest.config.json",
    "stats": "webpack --config webpack.config.prod.js --stats > stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FadeZhanger/react-typescript-template"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "express": "^4.17.1",
    "immer": "^3.1.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "ts-node": "^8.3.0"
  },
  "devDependencies": {
    "@supersede/webpack-progress-plugin": "^1.1.7",
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.0",
    "@types/mongoose": "^5.3.1",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/webpack": "^4.4.19",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-env": "^1.13.6",
    "@types/webpack-hot-middleware": "^2.16.4",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "3.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "4.0.0",
    "html-webpack-plugin": "3.2.0",
    "less": "^3.8.1",
    "less-loader": "^5.0.0",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.6",
    "react-hot-loader": "^4.3.12",
    "sass-loader": "^7.0.3",
    "style-loader": "0.23.1",
    "tslint": "^5.11.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3",
    "url-loader": "2.0.1",
    "webpack": "4.35.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3"
  }
}
