{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es5"
  },
  "awesomeTypescriptLoaderOptions": {
    "useCache": true,
    "forceIsolatedModules": true,
    "transpileOnly": true,
  }
}